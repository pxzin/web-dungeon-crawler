{
  "name": "web-dungeon-crawler",
  "version": "1.0.0",
  "description": "A web-based dungeon crawler game built with SvelteKit",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": ["game", "dungeon-crawler", "svelte", "typescript"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@unocss/preset-attributify": "^66.5.5",
    "@unocss/preset-icons": "^66.5.5",
    "@unocss/preset-uno": "^66.5.5",
    "@unocss/transformer-directives": "^66.5.5",
    "@unocss/transformer-variant-group": "^66.5.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.43.5",
    "svelte-check": "^4.0.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.5",
    "vite": "^7.2.2"
  }
}
